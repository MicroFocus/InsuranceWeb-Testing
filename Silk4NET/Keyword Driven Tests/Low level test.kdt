<?xml version="1.0" encoding="UTF-8"?>
<Keyword xmlns="http://www.borland.com/silk/Keyword">
    <InvokeKeyword name="Start default browser" />
    <InvokeKeyword name="Set text">
        <Argument value="//input[@id='login-form:email']" />
        <Argument value="john.smith@gmail.com" />
    </InvokeKeyword>
    <InvokeKeyword name="Set text">
        <Argument value="//input[@id='login-form:password']" />
        <Argument value="john" />
    </InvokeKeyword>
    <InvokeKeyword name="Click">
        <Argument value="//input[@id='login-form:login']" />
    </InvokeKeyword>
    <InvokeKeyword name="Select">
        <Argument value="//SELECT[@id='quick-link:jump-menu']" />
        <Argument value="Auto Quote" />
    </InvokeKeyword>
    <InvokeKeyword name="Click">
        <Argument value="//A[@href='http://demo.borland.com/InsuranceWebExtJS/index.jsf']" />
    </InvokeKeyword>
    <InvokeKeyword name="Click">
        <Argument value="//input[@id='logout-form:logout']" />
    </InvokeKeyword>
    <InvokeKeyword name="Close browser" />
</Keyword>